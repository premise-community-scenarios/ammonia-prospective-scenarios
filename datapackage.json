        "title": "Marc van der Meide",
        "email": "m.t.van.der.meide@cml.leidenuniv.nl"
    },
        {
        "title": "Romain Sacchi",
        "email": "r_s@me.com"
    }
    ],
    "dependencies": {
        "premise": ">=1.2.5"
    },
    "ecoinvent": {
        "version": "3.8",
        "system model": "cut-off"
    },
    "scenarios": {
        "Business As Usual": [
            {
                "model": "image",
                "pathway": "SSP2-Base"
            }
        ],
        "Sustainable development": [
            {
                "model": "image",
                "pathway": "SSP2-RCP26"
            }
        ]
    },
